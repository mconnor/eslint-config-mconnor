{
  "name": "eslint-config-mconnor",
  "author": "Mike Connor",
  "type": "module",
  "repository": {
    "url": "https://github.com/mconnor/eslint-config-mconnor"
  },
  "description": "various linting configs",
  "version": "1.0.5",
  "private": false,
  "imports": {
    "#*": "./src/lints/*"
  },
  "exports": {
    "./*": {
      "default": "./src/lints/*.js"
    },
    "./tailwind-lint": "./src/lints/tw.lint.config.mjs",
    "./turbo-config": "./src/lints/turbo.lint.config.mjs",
    "./next-lint": "./src/lints/next.mjs",
    "./ignores": "./src/lints/ignores.config.mjs",
    "./vue-config": "./src/lints/vue.lint.config.mjs",
    "./md-config": "./src/lints/md.lint.config.mjs",
    "./react-internal": "./src/lints/react-internal.mjs",
    "./ts-lint": "./src/lints/ts.lint.config.mjs",
    "./lit-config": "./src/lints/lit.lint.config.mjs",
    "./type-disable": "./src/lints/tscheck.disable.config.mjs",
    "./regex": "./src/lints/regex.lint.config.mjs",
    "./svelte5-app": "./src/lints/svelte5.lint.config.mjs",
    "./solid": "./src/lints/sold.lint.config.mjs",
    "./next-config": "./src/lints/next.mjs",
    "./simple-imports-sort": "./src/lints/simple-imports-sort.lint.config.mjs"
  },
  "scripts": {
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=9"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "latest",
    "@eslint/compat": "^1.3.1",
    "@eslint/css": "^0.9.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.6.0",
    "@next/eslint-plugin-next": "^15.3.5",
    "@typescript-eslint/parser": "^8.36.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-barrel-files": "^3.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-regexp": "^2.9.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.10.1",
    "eslint-plugin-turbo": "^2.5.4",
    "eslint-plugin-vue": "^10.3.0",
    "eslint-plugin-wc": "^3.0.1",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "svelte-eslint-parser": "^1.2.0",
    "tailwind-csstree": "^0.1.1",
    "typescript-eslint": "^8.36.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "engines": {
    "node": ">= 22.9.0"
  },
  "packageManager": "pnpm@10.13.1"
}
