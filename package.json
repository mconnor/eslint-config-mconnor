{
  "name": "eslint-config-mconnor",
  "author": "Mike Connor",
  "type": "module",
  "repository": {
    "url": "https://github.com/mconnor/eslint-config-mconnor"
  },
  "description": "various linting configs",
  "version": "2.0.3",
  "private": false,
  "imports": {
    "#*": "./src/*"
  },
  "exports": {
    "./tailwind-lint": {
      "default": "./tw.lint.config.mjs"
    },
    "./turbo-lint": {
      "default": "./turbo.lint.config.mjs"
    },
    "./next-lint": {
      "default": "./next.mjs"
    },
    "./vue-lint": {
      "default": "./vue.lint.config.mjs"
    },
    "./md-lint": {
      "default": "./md.lint.config.mjs"
    },
    "./react-internal-lint": {
      "default": "./react-internal.mjs"
    },
    "./ts-lint": {
      "default": "./ts.lint.config.mjs"
    },
    "./lit-lint": {
      "default": "./lit.lint.config.mjs"
    },
    "./type-disable": {
      "default": "./tscheck.disable.config.mjs"
    },
    "./regex-lint": {
      "default": "./regex.lint.config.mjs"
    },
    "./svelte-lint": {
      "default": "./svelte5.lint.config.mjs"
    },
    "./solid-lint": {
      "default": "./solid.lint.config.mjs"
    },
    "./simple-imports-lint": {
      "default": "./simple-imports-sort.lint.config.mjs"
    }
  },
  "scripts": {
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "license": "MIT",
  "peerDependencies": {
    "eslint": ">=9"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "latest",
    "@eslint/compat": "^1.3.1",
    "@eslint/css": "^0.10.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@eslint/json": "^0.13.0",
    "@eslint/markdown": "^7.0.0",
    "@next/eslint-plugin-next": "^15.3.5",
    "@typescript-eslint/parser": "^8.36.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-barrel-files": "^3.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-regexp": "^2.9.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.10.1",
    "eslint-plugin-turbo": "^2.5.4",
    "eslint-plugin-vue": "^10.3.0",
    "eslint-plugin-wc": "^3.0.1",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "svelte-eslint-parser": "^1.2.0",
    "tailwind-csstree": "^0.1.1",
    "typescript-eslint": "^8.36.0",
    "vue-eslint-parser": "^10.2.0"
  }
}
